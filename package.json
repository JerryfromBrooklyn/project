{
    "name":  "shmong-face-register",
    "version":  "1.0.0",
    "description":  "Face registration Lambda function for Shmong app",
    "main":  "index.js",
    "scripts":  {
                    "dev":  "node scripts/generate-timestamp.js \u0026\u0026 vite",
                    "setup":  "node scripts/generate-timestamp.js \u0026\u0026 vite",
                    "build":  "node scripts/generate-timestamp.js \u0026\u0026 vite build",
                    "preview":  "vite preview",
                    "test":  "node test-lambda-functions.js",
                    "s3-sync":  "aws s3 sync dist/ s3://shmong --delete --exclude \"photos/*\" --exclude \"face-images/*\"",
                    "s3-sync:cache":  "npm run s3-sync:html \u0026\u0026 npm run s3-sync:assets",
                    "s3-sync:html":  "aws s3 sync dist/ s3://shmong --delete --exclude \"*\" --include \"*.html\" --cache-control \"public, max-age=0, must-revalidate\" --content-type \"text/html; charset=utf-8\"",
                    "s3-sync:assets":  "aws s3 sync dist/assets/ s3://shmong/assets --delete --cache-control \"public, max-age=31536000, immutable\"",
                    "invalidate-cf":  "aws cloudfront create-invalidation --distribution-id E3OEKXFISG92UV --paths \"/*\"",
                    "deploy":  "npm run build \u0026\u0026 npm run s3-sync \u0026\u0026 npm run invalidate-cf",
                    "deploy:cache":  "npm run build \u0026\u0026 npm run s3-sync:cache \u0026\u0026 npm run invalidate-cf",
                    "release":  "npm version patch \u0026\u0026 npm run deploy:cache"
                },
    "author":  "",
    "license":  "ISC",
    "devDependencies":  {
                            "@vitejs/plugin-react":  "4.3.4",
                            "cross-env":  "^7.0.3",
                            "vite":  "6.2.5",
                            "vite-plugin-node-polyfills":  "0.23.0"
                        },
    "dependencies":  {
                         "@aws-sdk/client-cognito-identity-provider":  "3.782.0",
                         "@aws-sdk/client-dynamodb":  "3.782.0",
                         "@aws-sdk/client-rekognition":  "3.782.0",
                         "@aws-sdk/client-s3":  "3.782.0",
                         "@aws-sdk/lib-dynamodb":  "3.782.0",
                         "@aws-sdk/s3-request-presigner":  "3.782.0",
                         "@radix-ui/react-slot":  "1.2.0",
                         "@react-google-maps/api":  "2.20.6",
                         "@tailwindcss/postcss":  "4.1.3",
                         "autoprefixer":  "10.4.21",
                         "aws-sdk":  "2.1692.0",
                         "axios":  "1.8.4",
                         "clsx":  "2.1.1",
                         "cross-fetch":  "4.1.0",
                         "dotenv":  "16.4.7",
                         "file-saver":  "2.0.5",
                         "framer-motion":  "12.6.3",
                         "jszip":  "3.10.1",
                         "lucide-react":  "0.487.0",
                         "postcss":  "8.5.3",
                         "react":  "19.1.0",
                         "react-dom":  "19.1.0",
                         "react-dropzone":  "14.3.8",
                         "react-icons":  "5.5.0",
                         "react-router-dom":  "7.5.0",
                         "react-webcam":  "7.2.0",
                         "tailwind-merge":  "3.2.0",
                         "tailwindcss":  "3.3.3"
                     }
}
