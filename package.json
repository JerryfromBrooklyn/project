{
  "name": "shmong-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "clean-start": "node scripts/kill-node.js && npm run dev",
    "increment-build": "node scripts/increment-build.js",
    "prebuild": "npm run increment-build",
    "build": "vite build --config vite.config.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "netlify:dev": "netlify dev",
    "netlify:deploy": "netlify deploy",
    "netlify:deploy:prod": "netlify deploy --prod",
    "reindex-faces": "tsx scripts/reindex-faces.ts",
    "test-face": "tsx scripts/test-face-recognition.ts",
    "test-indexing": "tsx scripts/test-face-indexing.ts",
    "test-registration": "tsx scripts/test-face-registration.ts",
    "cleanup-faces": "tsx scripts/cleanup-faces.ts",
    "migrate-faces": "tsx scripts/migrate-faces.ts",
    "setup-aws": "node scripts/setup-aws.js",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "deploy:win": "powershell -ExecutionPolicy Bypass -File .\\deploy.ps1",
    "build-deploy": "npm run build && npm run deploy",
    "build-deploy:win": "npm run build && npm run deploy:win",
    "deploy-auto": "node scripts/deploy-auto.js",
    "build-deploy-auto": "npm run build && npm run deploy-auto"
  },
  "dependencies": {
