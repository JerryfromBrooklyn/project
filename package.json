    "amazon-cognito-identity-js": "6.3.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "16.4.7",
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.323.0",
    "node-fetch": "2.7.0",
